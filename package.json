{
  "name": "classy-node",
  "version": "2.6.8",
  "description": "Classy API wrapper",
  "scripts": {
    "lint": "./node_modules/.bin/eslint ./src",
    "test": "./node_modules/.bin/mocha --require @babel/register --file ./test/* && yarn coverage",
    "coverage": "./node_modules/.bin/babel-node ./node_modules/isparta/bin/isparta cover _mocha",
    "snyk-test": "./node_modules/.bin/snyk test",
    "snyk-unpublished": "./node_modules/.bin/snyk test-unpublished",
    "snyk-protect": "./node_modules/.bin/snyk protect",
    "prepublish": "yarn build",
    "build": "./node_modules/.bin/gulp",
    "snyk-wizard": "node ./node_modules/.bin/snyk wizard"
  },
  "keywords": [
    "classy",
    "classy.org",
    "api",
    "nonprofits",
    "donations",
    "fundraising"
  ],
  "homepage": "https://github.com/classy-org/classy-node",
  "author": "Classy, Inc. <support@classy.org> (https://classy.org)",
  "contributors": [
    "Matt Lingner <mlingner@gmail.com> (http://mattlingner.com)"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/classy-org/classy-node.git"
  },
  "bugs": "https://github.com/classy-org/classy-node/issues",
  "engines": {
    "node": "10.15.2"
  },
  "main": "dist/Classy/main.js",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "gulp-babel": "^8.0.0",
    "handlebars": "^4.2.0",
    "lodash": "^4.0.0",
    "pluralize": "^8.0.0",
    "request": "^2.74.0",
    "request-debug": "^0.2.0",
    "v8flags": "^3.1.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "colors": "^1.1.2",
    "coveralls": "^3.0.6",
    "del": "^5.1.0",
    "dotenv": "^8.1.0",
    "gulp": "^4.0.2",
    "gulp-add-src": "^1.0.0",
    "gulp-jshint": "^2.0.0",
    "gulp-mocha": "^7.0.1",
    "isparta": "^4.0.0",
    "jshint": "^2.9.1",
    "lolex": "^4.2.0",
    "mocha": "^6.2.0",
    "nock": "^11.3.1",
    "sinon": "^7.4.2",
    "sinon-chai": "^3.3.0",
    "snyk": "^1.15.0"
  }
}
